{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "strict": false,
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "declarationMap": true,
  },
//   "files": [ "./install/install.ts", "./install/parseEnv.ts" ],
  "include": [ "./install/*.ts", "./lib/**/*.ts", "./lib/src/**/*.ts" ]
}
